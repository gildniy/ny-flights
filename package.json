{
  "name": "ny-flights",
  "version": "1.0.0",
  "description": "A simple classified management system built with React, Webpack and Babel",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "watch": "webpack-dev-server --open --mode development",
    "start": "node server.js",
    "heroku-postbuild": "webpack -p",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "keywords": [
    "New York",
    "flights",
    "listing",
    "currency"
  ],
  "author": "Gildas Niyigena",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "babel-loader": "^8.0.6",
    "coveralls": "^3.0.9",
    "css-loader": "^3.4.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "node-sass": "^4.13.0",
    "react-test-renderer": "^16.12.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.2",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap": "^4.4.1",
